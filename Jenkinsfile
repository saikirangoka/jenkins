pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    sh """
                    echo "Hello World"
                    """
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    sh """
                    echo "Hello sai"
                    """
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    sh """
                        echo "Hello kiran"
                    """
                }
            }
        }
    }
}